{
  "name": "mezcalmos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "emulator": "cross-env FIREBASE_DATABASE_EMULATOR_HOST=localhost:9000 FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 firebase emulators:start --export-on-exit=../test/data/newDummyData",
    "emulator:onlyFunctions": "firebase emulators:start --only functions",
    "supervisor": "cross-env node ../supervisor/index.js",
    "loadData": "cross-env FIREBASE_DATABASE_EMULATOR_HOST=localhost:9000 FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 node ../test/loadData.js",
    "serve:customers": "vue-cli-service serve src/customers/main.js",
    "test:customers": "vue-cli-service serve --mode test --port 8080 src/customers/main.js",
    "emulate:customers": "vue-cli-service serve --mode emulate --port 8080 src/customers/main.js",
    "emulate:customers:onlyFunctions": "vue-cli-service serve --mode emulateOnlyFunctions --port 8080 src/customers/main.js",
    "deploy:customers": "node scripts/run --command deploy --app customers --mode production",
    "deploy:customers:test": "node scripts/run --command deploy --app customers --mode test",
    "serve:taxis": "vue-cli-service serve --port 8081 src/taxis/main.js",
    "test:taxis": "vue-cli-service serve --mode test --port 8081 src/taxis/main.js",
    "emulate:taxis": "vue-cli-service serve --mode emulate --port 8081 src/taxis/main.js",
    "emulate:taxis:onlyFunctions": "vue-cli-service serve --mode emulateOnlyFunctions --port 8081 src/taxis/main.js",
    "deploy:taxis": "node scripts/run --command deploy --app taxis --mode production",
    "deploy:taxis:test": "node scripts/run --command deploy --app taxis --mode test",
    "serve:deliveries": "vue-cli-service serve --port 8082 src/deliveries/main.js",
    "test:deliveries": "vue-cli-service serve --mode test --port 8082 src/deliveries/main.js",
    "emulate:deliveries": "vue-cli-service serve --mode emulate --port 8082 src/deliveries/main.js",
    "emulate:deliveries:onlyFunctions": "vue-cli-service serve --mode emulateOnlyFunctions --port 8082 src/deliveries/main.js",
    "deploy:deliveries": "node scripts/run --command deploy --app deliveries --mode production",
    "deploy:deliveries:test": "node scripts/run --command deploy --app deliveries --mode test",
    "serve:admin": "vue-cli-service serve --port 9090 src/admin/main.js",
    "test:admin": "vue-cli-service serve --mode test --port 9090 src/admin/main.js",
    "emulate:admin": "vue-cli-service serve --mode emulate --port 9090 src/admin/main.js",
    "emulate:admin:onlyFunctions": "vue-cli-service serve --mode emulateOnlyFunctions --port 9090 src/admin/main.js",
    "deploy:admin": "node scripts/run --command deploy --app admin --mode production",
    "deploy:admin:test": "node scripts/run --command deploy --app admin --mode test",
    "serve:all": "npm-run-all --parallel serve:taxis serve:customers serve:admin",
    "test:all": "npm-run-all --parallel test:taxis test:customers test:admin",
    "emulate:allApps": "npm-run-all --parallel emulate:taxis emulate:customers emulate:admin",
    "emulate:allApps:onlyFunctions": "npm-run-all --parallel emulate:taxis:onlyFunctions emulate:customers:onlyFunctions emulate:admin:onlyFunctions",
    "emulate:all": "npm-run-all --parallel emulate:allApps emulator",
    "emulate:all:supervisor:test": "npm-run-all --parallel emulate:allApps emulator supervisor:test",
    "emulate:all:wdata": "npm-run-all --parallel emulate:all emulator:loadData",
    "emulate:all:onlyFunctions": "npm-run-all --parallel emulate:allApps:onlyFunctions emulator:onlyFunctions",
    "deploy:all": "node scripts/run --command deploy --app all --mode production",
    "deploy:all:test": "node scripts/run --command deploy --app all --mode test",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.6.5",
    "firebase-admin": "^9.11.0",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "gsap": "^3.6.0",
    "is-svg": "^4.3.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^5.0.0",
    "register-service-worker": "^1.7.1",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.7",
    "vue-i18n": "^8.22.4",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.4.9",
    "vue2-gmap-custom-marker": "^6.0.0",
    "vue2-google-maps": "^0.10.7",
    "vue2-touch-events": "^3.2.2",
    "vuetify": "^2.4.6",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "firebase": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.11",
    "workbox-cli": "^6.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
